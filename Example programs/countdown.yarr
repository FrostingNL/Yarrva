fleet Program {	
	ship countDown(doubloon n, doubloon m) {
		doubloon c be n +m, Arrr!
		whirlpool(c above 0) {
			booty bo be "Hello!", Arrr!
			parrot c, Arrr!
			plunder c, Arrr!
		}
		avast (n+m), Arrr!
	}
	
	flagship() {
		doubloon a be 5, Arrr!
		doubloon b be 15, Arrr!
		parrot (countDown(a,b)), Arrr!
	}
} 