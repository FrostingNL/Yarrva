fleet Program {
	
	doubloonShip function(doubloon a, doubloon b) {
		orderShip comp(doubloon x, doubloon y) {
			order temp be Nay, Arrr!
			parley(x be y) {
				temp be Aye, Arrr!
			}
			avast temp, Arrr!
		}

		order d be Nay, Arrr!
		doubloon c be a+b, Arrr!
		b be c - b, Arrr!

		d be comp(a,b), Arrr!
		parley(d) {
			parrot Aye, Arrr!
		}
		heave {
			parrot Nay, Arrr!
		}

		navigate(b be 0. b be below a. gift b) {
			parrot b, Arrr!
			plunder c, Arrr!
		}
		avast c, Arrr!
	}

	flagship() {
		doubloon e be 1, Arrr!
		doubloon r be 2, Arrr!
		parrot function(e,r), Arrr!
	}
}