fleet Fleet {
	doubloon a be 1, Arrr!
	order b be Aye, Arrr!

	doubloonShip add(doubloon x, doubloon y) {
		doubloon temp be x + y, Arrr!
		avast temp, Arrr!
	}

	doubloonShip sub(doubloon x, doubloon y) {
		doubloon temp be x - y, Arrr!
		avast temp, Arrr!
	}

	flagship() {
		doubloon five be 5, Arrr!
		parley(b) {
			parrot (add(a, five)), Arrr!
		}
	}
}