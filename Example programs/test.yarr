fleet showDoubloonFunc {

	doubloonShip addDiffNumbers(doubloon x, doubloon y) {
		parley(x be y) {
			avast 1, Arrr!
		} heave {
			doubloon sum be (x+y), Arrr!
			avast sum, Arrr!
		}
	}


	flagship() {
		doubloon x be 7, Arrr!
		doubloon y be 1, Arrr!
		parrot addDiffNumbers(x, y), Arrr!

		doubloon a be 2, Arrr!
		doubloon b be 2, Arrr!
		parrot addDiffNumbers(a, b), Arrr!
	}
}